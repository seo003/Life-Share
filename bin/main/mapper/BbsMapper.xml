<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespaceì— dao(mapper.java) -->
<mapper namespace="com.example.demo.mapper.BbsMapper">

	<insert id="bbsWrite" parameterType="com.example.demo.dto.BbsDTO">
		INSERT INTO bbs(bbsId, userId, bbsContent) values (#{bbsId}, #{userId}, #{bbsContent})
	</insert>
	
	<!-- resultType="List"-->
	<select id="getMyBbsAll" parameterType="com.example.demo.dto.BbsDTO" resultType="com.example.demo.dto.BbsDTO">
		SELECT * FROM bbs WHERE userId = #{userId}
	</select>
</mapper>